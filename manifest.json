{
  "applications": {
    "gecko": {
      "id": "FANDOM-Monaco@tmkch.github.io"
    }
  },
  "manifest_version": 2,
  "name": "FANDOM Monaco",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.wikia.com/wiki/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["page.js"],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "permissions": [
    "*://*.wikia.com/*",
    "webRequest",
    "tabs"
  ]
}